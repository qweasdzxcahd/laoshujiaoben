local fpsCounter = 0
local fpsLastTime = tick()
local fpsText = "è®¡ç®—ä¸­..."

spawn(function()
    while wait() do
        fpsCounter = fpsCounter + 1
        
        if tick() - fpsLastTime >= 1 then
            fpsText = string.format("%.1f FPS", fpsCounter)
            fpsCounter = 0
            fpsLastTime = tick()
        end
    end
end)

Taba:Paragraph({
    Title = "æ€§èƒ½ä¿¡æ¯",
