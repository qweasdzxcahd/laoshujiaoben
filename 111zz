-- ========== ESPæ ‡ç­¾é¡µ ==========
local espEnabled = false
local espObjects = {}
local refreshConnection = nil

local espSettings = {
    showName = false,
    showDistance = false,
    showHealth = false,
    showBox = false
}

function clearAllESP()
    for player, objects in pairs(espObjects) do
        for _, obj in pairs(objects) do
            if obj and obj.Parent then
                obj:Destroy()
            end
        end
    end
    espObjects = {}
end

function updateAllESP()
    clearAllESP()
    
    if not espEnabled then
        return
    end
    
    for _, player in ipairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer then
            -- ç®€åŒ–çš„ESPåˆ›å»ºé€»è¾‘
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                -- è¿™é‡Œå¯ä»¥æ·»åŠ å®Œæ•´çš„ESPåˆ›å»ºä»£ç 
            end
        end
    end
end

Toggle(Tab2, "å¯ç”¨ESPæ€»å¼€å…³", false, function(value)
    espEnabled = value
    
    if value then
        updateAllESP()
    else
        clearAllESP()
    end
end)

Toggle(Tab2, "æ˜¾ç¤ºç©å®¶åç§°", false, function(value)
    espSettings.showName = value
    if espEnabled then
        updateAllESP()
    end
end)

Toggle(Tab2, "æ˜¾ç¤ºç©å®¶è·ç¦»", false, function(value)
    espSettings.showDistance = value
    if espEnabled then
        updateAllESP()
    end
end)

Toggle(Tab2, "æ˜¾ç¤ºç©å®¶è¡€é‡", false, function(value)
    espSettings.showHealth = value
    if espEnabled then
        updateAllESP()
    end
end)

Toggle(Tab2, "æ˜¾ç¤ºç©å®¶æ–¹æ¡†", false, function(value)
    espSettings.showBox = value
    if espEnabled then
        updateAllESP()
    end
end)

